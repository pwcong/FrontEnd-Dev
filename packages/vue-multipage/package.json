{
  "name": "vue-multipage",
  "version": "1.0.0",
  "description": "An vue multipage framework",
  "main": "./src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "_dev": "webpack-dev-server --open",
    "dev": "cross-env ENTRY=/ npm run _dev",
